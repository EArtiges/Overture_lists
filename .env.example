# Overture Admin Boundary List Builder - Environment Configuration

# Parquet Data Path
# Can be S3, HTTP, or local file path
OVERTURE_PARQUET_PATH=s3://overturemaps-us-west-2/release/2024-11-13.0/theme=admins/type=*/*.parquet

# Data Storage Directory
# Directory where list JSON files are stored
LIST_DATA_DIR=./list_data

# Streamlit Configuration (Docker)
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0
STREAMLIT_SERVER_HEADLESS=true
STREAMLIT_BROWSER_GATHER_USAGE_STATS=false
